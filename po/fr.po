msgid ""
"\n"
" Show global DNS configuration:\n"
"   ipa dnsconfig-show\n"
msgstr ""
"\n"
"Afficher la configuration DNS globale :\n"
"   ipa dnsconfig-show\n"

msgid ""
"\n"
