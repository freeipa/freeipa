"[siz[\"m\"] [hp[\"m\"] [vp[\"m\"]]]]\"\n"
"    где:\n"
"       d1:     [0 .. 90]            (градусы широты)\n"
"       d2:     [0 .. 180]           (градусы долготы)\n"
"       m1, m2: [0 .. 59]            (минуты широты / долготы)\n"
"       s1, s2: [0 .. 59.999]        (секунды широты / долготы)\n"
"       alt:    [-100000.00 .. 42849672.95] BY .01 (высота в метрах)\n"
