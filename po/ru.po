msgstr "    * 8 - removeFromCRL\n"

#: ipaserver/plugins/cert.py:148
msgid "    * 9 - privilegeWithdrawn\n"
msgstr "    * 9 - privilegeWithdrawn\n"

#: ipaserver/plugins/cert.py:149
